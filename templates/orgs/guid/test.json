 &quot;user_id&quot;: 109, &quot;staff&quot;: &quot;Marwan12&quot;, &quot;name&quot;: &quot;جمعية دجلة لحماية البيئة&quot;, &quot;name_en_ku&quot;: &quot;Dijlah Association for Environmental Protection&quot;, &quot;short_cut&quot;: &quot;D.A.E.P&quot;, &quot;org_type&quot;: &quot;org&quot;, &quot;logo&quot;: &quot;org_logos/Degla_1_-_Mistefa_Goher_21.png&quot;, &quot;message&quot;: &quot;&lt;table cellspacing=&quot;0&quot; style=&quot;border-collapse:collapse; width:151px&quot;&gt;

